<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- /** @author FedeBeron * @version 1.0 */ -->

<hibernate-mapping>
    <class name="com.eBolivar.domain.TipoEspacio" table="TIPOS_ESPACIO">
	
          <id name="id" column="TPO_ID" type="java.lang.Integer" >
			 <generator class="increment"/>
		  </id>
          <property name="nombre"  column="TPO_NOMBRE"  type="java.lang.String" />
		  
    </class>
</hibernate-mapping>
