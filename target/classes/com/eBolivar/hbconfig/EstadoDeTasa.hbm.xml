<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eBolivar.domain.EstadoDeTasa" table="ESTADOS_TASA">

        <id name="id" column="EST_ID" type="java.lang.Integer"/>
        <property name="descripcion" column="EST_DESCRIPCION" type="java.lang.String" />

    </class>
</hibernate-mapping>
