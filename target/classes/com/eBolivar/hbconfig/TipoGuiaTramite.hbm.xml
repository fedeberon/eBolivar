<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- /** @author FedeBeron * @version 1.0 */ -->

<hibernate-mapping>
    <class name="com.eBolivar.domain.TipoGuiaTramite" table="TIPOS_GUIAS_TRAMITES">
	
          <id name="codigo" column="TG_ID" type="java.lang.Integer" >
			<generator class="increment"/>  
		  </id>
          <property name="nombre"  column="TG_NOMBRE"  type="java.lang.String" />
		  
    </class>
</hibernate-mapping>
