<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eBolivar.domain.Dependencia" table="DEPENDENCIAS">

        <id name="id" column="DEP_ID" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property name="descripcionDependencia"  column="DEP_DESCRIPCION_DEPENDENCIA"  type="java.lang.String" />
        <property name="idDependencia"  column="DEP_ID_DEPENDENCIA"  type="java.lang.String" />
    </class>
</hibernate-mapping>