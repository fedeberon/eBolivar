<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- /** @author JoseAlv * @version 1.0 */ -->

<hibernate-mapping>
	<class name="com.eBolivar.domain.DetalleFactura" table="DETALLE_FACTURA">

			
		<composite-id>
			 <key-property   name="idFactura" column="DET_ID_FACTURA"  type="java.lang.String" />
			 <key-property   name="item" column="DET_ITEM" type="java.lang.Integer" />
		</composite-id>
		
		<property name="tipoRegistro" column="DET_TIPO_REGISTRO" type="java.lang.Integer" />
		<property name="detalle" column="DET_DETALLE" type="java.lang.String" />

	</class>
</hibernate-mapping>
