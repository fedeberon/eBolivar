<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- /** @author JoseAlv * @version 1.0 */ -->

<hibernate-mapping>
	<class name="com.eBolivar.domain.NotificacionPorBien" table="NOTIFICACION_POR_BIEN">
 	
 		<composite-id>
			 <key-property   name="codigoElectronicoPago" column="NPB_CODIGO_ELECTRONICO_PAGO" type="java.lang.Long" />
			 <key-property   name="numeroNotificacion" column="NPB_NUMERO_NOTIFICACION" type="java.lang.Integer" />
		</composite-id>
 
 		<property name="tipoRegistro" column="DET_TIPO_REGISTRO" type="java.lang.Integer" />
		<property name="notificacion" column="NPB_NOTIFICACION" type="java.lang.String" />

	</class>
</hibernate-mapping>
