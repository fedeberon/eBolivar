<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>

    <class name="com.eBolivar.domain.Parametro" table="PARAMETROS">
        <id name="id" type="java.lang.String" column="PAR_ID" />
        <property name="nombre" type="java.lang.String" column="PAR_NAME" />
        <property name="valor" type="java.lang.String" column="PAR_VALUE" />
        <property name="descripcion" type="java.lang.String" column="PAR_DESCRIPTION" />
    </class>
    
</hibernate-mapping>



    
    