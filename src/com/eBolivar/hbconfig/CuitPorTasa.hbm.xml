<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.eBolivar.domain.PadronAsociado" table="CUIT_POR_TASAS">
        <id name="id" column="CPT_ID" type="java.lang.Integer" >
            <generator class="increment"/>
        </id>
        <property name="padron"  column="CPT_PADRON"  type="java.lang.String" />
        <property name="leyendaDelTributo"  column="CPT_LEYENDA"  type="java.lang.String" />
        <many-to-one name="persona" class="com.eBolivar.domain.Persona" column="PAD_PER_ID" lazy="false"/>
    </class>
</hibernate-mapping>
